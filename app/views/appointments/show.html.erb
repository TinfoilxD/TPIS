<div id="form-wrapper">
  <div class="row">
    <div class="col-xs-6 col-sm-8 col-md-6 col-md-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-2 col-lg-offset-3">

      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">Instructor Candidate Profile </h3>

        </div>

        <div class="panel-body" >
          <div class="row">
            <div class="col-lg-14">
              <table class="profile-table table-profile-information ">
                <tbody>
<tr>
 <td> <strong>Start:</strong></td>
 <td> <%= @appointment.start.strftime('%B %d, %Y  %l:%M %p') %></td>
</tr>

<tr>
 <td> <strong>End:</strong></td>
  <td><%= @appointment.end.strftime('%B %d, %Y  %l:%M %p') %></td>
</tr>

<tr>
  <td><strong>Faculty:</strong></td>
  <td><%=@faculty.full_name%></td>
</tr>

<tr>
 <td> <strong>Candidate:</strong></td>
  <td><%=@candidate.full_name%></td>
</tr>

<tr>
 <td> <strong>Candidate Phone:</strong></td>
 <td><%=@candidate.phone%></td>
</tr>


<tr>
  <td><strong>Interview Complete:</strong></td>
  <td><%= yesno(@appointment.complete) %></td>
</tr>

<tr>
 <td> <strong>Interview Comments:</strong></td>
  <td><%=@appointment.comments%></td>
</tr>


<tr>
 <td> <%=link_to 'View Application', "/application_forms/#{@application_form.id}", class:'btn btn-info btn-S' %>|<%= link_to 'Edit', edit_appointment_path(@appointment), class:'btn btn-info btn-S'%> </td>
  <td></td>
</tr>
</tbody>
  <p><%= link_to 'Back', url_for(:back), class:'btn btn-default btn-S'%> <%= link_to 'Interview', interview_questions_path(@appointment), class:'btn btn-primary btn-S pull-right RbtnMargin'%> </p>
              </table>
                </div>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>

