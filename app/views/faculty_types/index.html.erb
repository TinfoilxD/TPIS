<p id="notice"><%= notice %></p>

<h1>Faculty Types</h1>

<table>
  <thead>
  <tr>
    <th>Faculty Type Title</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @faculty_types.each do |faculty_type| %>
      <tr>
        <td><p><%= faculty_type.job_title %></p></td>
        <td><p><%= link_to 'Show', faculty_type, class:'btn btn-default btn-xs' %></p></td>
        <td><p><%= link_to 'Edit', edit_faculty_type_path(faculty_type), class:'btn btn-default btn-xs' %></p></td>
        <td><p><%= link_to 'Delete', faculty_type, method: :delete, class:'btn btn-default btn-xs', data: { confirm: 'Are you sure?' } %></p></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>
<p>
  <%= link_to 'New Faculty Type', new_faculty_type_path, class:'btn btn-default btn-S' %>
  |
  <%= link_to 'Back', admin_configurations_path, class:'btn btn-default btn-S' %>
</p>
