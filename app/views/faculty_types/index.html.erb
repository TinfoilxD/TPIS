<p id="notice"><%= notice %></p>

<h1>Faculty Types</h1>

<table>
  <thead>
    <tr>
      <th>Job title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @faculty_types.each do |faculty_type| %>
      <tr>
        <td><%= faculty_type.job_title %></td>
        <td><%= link_to 'Show', faculty_type %></td>
        <td><%= link_to 'Edit', edit_faculty_type_path(faculty_type) %></td>
        <td><%= link_to 'Destroy', faculty_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Faculty Type', new_faculty_type_path %>
