
<div id="form-wrapper">
  <div class="row">
    <div class="col-xs-6 col-sm-8 col-md-6 col-md-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-2 col-lg-offset-3">

      <div class="panel panel-primary">
        <div class="panel-heading">
          <h3 class="panel-title">Instructor Candidate Profile</h3>
        </div>

        <div class="panel-body" >
          <div class="row">
            <div class="col-lg-14">
              <table class="profile-table table-profile-information ">
                <tbody>
 <tr>

 <tr>
   <td><strong>Training Center:</strong></td>
   <td><%= @faculty.training_center&.training_center_name %></td>
 </tr>


 <tr>
   <td><strong>Faculty type:</strong></td>
   <td><%= @faculty.faculty_type&.job_title %></td>
 </tr>



<td>  <strong>First name:</strong></td>
 <td>  <%= @faculty.first_name %> </td>

</tr>

 <tr>
 <td> <strong>Last name:</strong> </td>
<td>  <%= @faculty.last_name %></td>
 </tr>



<tr>
  <td><strong>Email:</strong></td>
 <td> <%= @faculty.email %></td>
</tr>

<tr>
 <td><strong>Phone:</strong></td>
  <td><%= @faculty.phone %></td>
</tr>

<tr>
 <td> <strong>Office address:</strong></td>
  <td><%= @faculty.office_address %></td>
</tr>

<tr>
  <td><strong>State:</strong></td>
  <td><%= @faculty.state&.state_name %></td>
</tr>

<tr>
  <td><strong>Zipcode:</strong></td>
  <td><%= @faculty.zip_code %></td>
</tr>

      </tbody>
  </table>

 <div id="panel-footer">

  <span class="pull-right">

<%= link_to 'Edit', edit_faculty_path(@faculty), class:'btn btn-info' %> |

    <%if user_signed_in? and current_user.tcf_admin? then%>
<%= link_to 'continue', tcf_admin_landing_path, class:'btn btn-primary' %>
       <%end%>
    <%if user_signed_in? and current_user.tcf? then%>
<%= link_to 'continue', tcf_admin_landing_path, class:'btn btn-primary' %>
       <%end%>

    <%if user_signed_in? and current_user.system_admin? then%>
<%= link_to 'Back', faculties_path, class:'btn btn-primary' %>
<%end%>


            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>